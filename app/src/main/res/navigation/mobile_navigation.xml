<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="app.ditodev.decedeevent.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="app.ditodev.decedeevent.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_finished"
        android:name="app.ditodev.decedeevent.ui.event.finished_event.FinishedEventFragment"
        android:label="Finished"
        tools:layout="@layout/fragment_finished_event" />

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="app.ditodev.decedeevent.ui.event.upcoming_event.UpcomingEventFragment"
        android:label="Upcoming"
        tools:layout="@layout/fragment_upcoming_event">
    </fragment>
</navigation>